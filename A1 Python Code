# create list 
# ask user for input
# append the new list to the data/2d array



data = []
with open(file_path, 'r') as f:
    for line in f.readlines():
        inner_data=[]
        for element in line.split(","):
            inner_data.append(element.strip())
        data.append(inner_data)
#print(data[1][1])
#print(data[0][0])
print(data)



exit()
